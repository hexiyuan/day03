<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0"
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <servlet-name>RegisterUIServlet</servlet-name>
    <servlet-class>cn.itcast.web.UI.RegisterUIServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>RegiseterServlet</servlet-name>
    <servlet-class>cn.itcast.web.controller.RegiseterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>loginUIServlet</servlet-name>
    <servlet-class>cn.itcast.web.UI.loginUIServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>cn.itcast.web.controller.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>cn.itcast.web.controller.LogoutServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckRegiseterServlet</servlet-name>
    <servlet-class>cn.itcast.web.controller.CheckRegiseterServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>CheckServlet</servlet-name>
    <servlet-class>cn.itcast.web.controller.CheckServlet</servlet-class>
  </servlet>







  <servlet-mapping>
    <servlet-name>RegisterUIServlet</servlet-name>
    <url-pattern>/servlet/RegisterUIServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegiseterServlet</servlet-name>
    <url-pattern>/servlet/RegiseterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>loginUIServlet</servlet-name>
    <url-pattern>/servlet/loginUIServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/servlet/LogoutServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckRegiseterServlet</servlet-name>
    <url-pattern>/servlet/CheckRegiseterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>CheckServlet</servlet-name>
    <url-pattern>/servlet/CheckServlet</url-pattern>
  </servlet-mapping>

</web-app>